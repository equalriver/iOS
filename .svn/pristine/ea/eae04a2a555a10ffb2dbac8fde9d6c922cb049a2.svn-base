#pragma mark - set
- (void)setCourse:(WLKTCourse *)course{
    _course = course;
    if (course.introduce.length > 0 && ![self.detailImageArr containsObject:@"kcjs"]) {
        [self.detailImageArr addObject:@"kcjs"];
        [self.introTitleArr addObject:@"课程介绍"];
        [self.courseDetailHtmlStringArr addObject:course.introduce];
        [self.isCourseDetailMoreDic setObject:@(NO) forKey:@(self.detailImageArr.count - 1)];
    }
    if (course.teach_content.length > 0 && ![self.detailImageArr containsObject:@"jxap"]) {
        [self.detailImageArr addObject:@"jxap"];
        [self.introTitleArr addObject:@"教学安排"];
        [self.courseDetailHtmlStringArr addObject:course.teach_content];
        [self.isCourseDetailMoreDic setObject:@(NO) forKey:@(self.detailImageArr.count - 1)];
    }
    if (course.course_feature.length > 0 && ![self.detailImageArr containsObject:@"kcts"]) {
        [self.detailImageArr addObject:@"kcts"];
        [self.introTitleArr addObject:@"课程特色"];
        [self.courseDetailHtmlStringArr addObject:course.course_feature];
        [self.isCourseDetailMoreDic setObject:@(NO) forKey:@(self.detailImageArr.count - 1)];
    }
    if (course.suitable.length > 0 && ![self.detailImageArr containsObject:@"syxy"]) {
        [self.detailImageArr addObject:@"syxy"];
        [self.introTitleArr addObject:@"适用学员"];
        [self.courseDetailHtmlStringArr addObject:course.suitable];
        [self.isCourseDetailMoreDic setObject:@(NO) forKey:@(self.detailImageArr.count - 1)];
    }
    if (course.teach_target.length > 0 && ![self.detailImageArr containsObject:@"jxmb"]) {
        [self.detailImageArr addObject:@"jxmb"];
        [self.introTitleArr addObject:@"教学目标"];
        [self.courseDetailHtmlStringArr addObject:course.teach_target];
        [self.isCourseDetailMoreDic setObject:@(NO) forKey:@(self.detailImageArr.count - 1)];
    }

}

#pragma mark - get
- (CLLocationManager *)locationManager{
    if (!_locationManager) {
        _locationManager = [[CLLocationManager alloc] init];
        // 版本适配
        if ([[UIDevice currentDevice].systemVersion floatValue] >= 8.0) {
            [_locationManager requestWhenInUseAuthorization];
        }
        //设置代理
        _locationManager.delegate = self;
        //设置定位精度
        _locationManager.desiredAccuracy = kCLLocationAccuracyBest;
        //定位频率,每隔多少米定位一次
        _locationManager.distanceFilter = 10.0;//十米定位一次
    }
    return _locationManager;
}
