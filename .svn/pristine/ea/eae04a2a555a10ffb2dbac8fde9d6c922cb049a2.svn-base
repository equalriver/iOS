#import "WLKTCourseDetailVC.h"
#import "WLKTCourseDetailHeader.h"

@interface WLKTCourseDetailVC ()<UITableViewDelegate, UITableViewDataSource, CourseDetailBottomButtonDelegate, CourseDetailPopoverDelegate, CourseDetailAppointmentDelegate, CourseDetailHeadIndexesDelegate, LGPhotoPickerBrowserViewControllerDataSource, LGPhotoPickerBrowserViewControllerDelegate, WLKTCourseDetailPhotoDelegate, WLKTCourseDetailVRPhotoDelegate, WLKTCourseDetailTeacherDelegate, WLKTCourseDetailOtherCourseDelegate, WLKTCourseDetailGoTopDelegate, WLKTCourseDetailEvaluationStarDelegate, WLKTLoginCoordinatorDelegate, UIPopoverPresentationControllerDelegate, WLKTCourseDetailIntroDelegate, CLLocationManagerDelegate>
@property (strong, nonatomic) UIButton *backButton;
@property (strong, nonatomic) UIButton *shareButton;
@property (strong, nonatomic) UIButton *phoneButton;
@property (strong, nonatomic) UIButton *popoverButton;
@property (strong, nonatomic) WLKTCourseDetailBottomButtons *bottomBtns;
@property (strong, nonatomic) WLKTCourseDetailHeadIndexes *headIndexes;
@property (strong, nonatomic) UILabel *tableViewFooterlabel;
@property (nonatomic, strong) CLLocationManager *locationManager;

@property (strong, nonatomic) WLKTCourse *course;
@property (strong, nonatomic) WLKTCourseDetailData *courseDetailData;
@property (copy, nonatomic) NSArray<WLKTCourseDetailNewPhoto *> *photoArray;
@property (copy, nonatomic) NSArray<WLKTCourseDetailNewTeacher *> *teacherArr;
@property (copy, nonatomic) NSArray<WLKTCourseDetailNewAbout_list *> *about_listArr;
@property (copy, nonatomic) NSArray<WLKTCourseDetailNewHot_school *> *hotSchoolArr;

@property (copy, nonatomic) NSString *courseId;
@property (nonatomic) BOOL isHidden;
@property (strong, nonatomic) NSMutableArray *detailImageArr;
@property (strong, nonatomic) NSMutableArray *introTitleArr;
@property (strong, nonatomic) NSMutableArray *courseDetailHtmlStringArr;
@property (copy, nonatomic) NSArray *detailTitleColorArr;
@property (copy, nonatomic) NSArray *sectionTitleArr;
@property (nonatomic, assign) LGShowImageType showType;
@property (nonatomic, strong) NSMutableArray *LGPhotoPickerBrowserPhotoArray;
@property (nonatomic) CGFloat currentAlpha;
@property (strong, nonatomic) NSMutableArray *childCoordinator;
@property (copy, nonatomic) void (^loginBlock)(void);
@property (strong, nonatomic) NSMutableDictionary *courseDetailHeightDic;
@property (nonatomic) CGFloat insetY;
@property (strong, nonatomic) NSMutableDictionary<NSNumber *, NSNumber *> *isCourseDetailMoreDic;
@property (nonatomic, assign) CLLocationCoordinate2D currentCoordinate;

@end

@implementation WLKTCourseDetailVC
- (instancetype)initWithCourseId:(NSString *)courseId
{
    self = [super init];
    if (self) {
        self.courseId = [courseId copy];
    }
    return self;
}

- (void)viewDidLoad {
    [super viewDidLoad];
    [self loadData];
    [self.view addSubview:self.tableView];
    [self.view addSubview:self.headIndexes];
    [self.view addSubview:self.bottomBtns];
    [self.view addSubview:self.backButton];
    [self.view addSubview:self.shareButton];
    [self.view addSubview:self.phoneButton];
    [self.view addSubview:self.popoverButton];
    [self makeConstraints];
    self.isHidden = YES;
    self.insetY = 110.0;
    [self setCLLocation];
    WS(weakSelf);
    [WLKTTableviewRefresh tableviewRefreshHeaderWithTaget:self.tableView request:^{
        [weakSelf loadData];
    }];
}

-(void)viewWillAppear:(BOOL)animated{
    [super viewWillAppear:animated];
    //启动跟踪定位
    [self.locationManager startUpdatingLocation];
    //截屏
    [[NSNotificationCenter defaultCenter] addObserver:self
                                             selector:@selector(userDidTakeScreenshot:)
                                                 name:UIApplicationUserDidTakeScreenshotNotification object:nil];
    if (IsIOS_11_Later) {
        if (!IsIphoneX) {
            self.tableView.contentInsetAdjustmentBehavior = UIScrollViewContentInsetAdjustmentNever;
        }
    }
    else{
        self.automaticallyAdjustsScrollViewInsets = NO;
    }
    [self.navigationController.navigationBar setBackgroundImage:[UIImage imageWithColor:[UIColor colorWithWhite:1.0 alpha:self.currentAlpha >= 1 ? 0.99 : self.currentAlpha]] forBarMetrics:UIBarMetricsDefault];
    [self.navigationController.navigationBar setShadowImage:[UIImage imageWithColor:[UIColor colorWithWhite:14/16.0 alpha:self.currentAlpha >= 1 ? 0.99 : self.currentAlpha]]];
    self.navigationController.navigationBar.titleTextAttributes = @{NSForegroundColorAttributeName: [UIColor colorWithWhite:1.0 alpha:self.currentAlpha]};
    self.navigationItem.hidesBackButton = YES;
    
}

- (void)viewDidAppear:(BOOL)animated{
    [super viewDidAppear:animated];
    if (self.tableView.contentOffset.y < 1) {
        [self.navigationController setNavigationBarHidden:YES animated:NO];
    }
    if (self.course) {
        !self.completion ?: self.completion();
    }
}

- (void)viewWillDisappear:(BOOL)animated{
    [super viewWillDisappear:animated];
//    if (IsIOS_11_Later) {
        self.navigationController.navigationBar.titleTextAttributes = @{NSForegroundColorAttributeName: KMainTextColor_3};
        [self.navigationController.navigationBar setBackgroundImage:[UIImage imageWithColor:kNavBarBackgroundColor] forBarMetrics:UIBarMetricsDefault];
        [self.navigationController.navigationBar setShadowImage:[UIImage imageWithColor:kNavBarShadowImageColor]];
    [self.navigationController setNavigationBarHidden:NO animated:NO];
//    }
    [[NSNotificationCenter defaultCenter]removeObserver:self name:UIApplicationUserDidTakeScreenshotNotification object:nil];
}

- (void)dealloc{
    [[NSNotificationCenter defaultCenter]removeObserver:self];
}

#pragma mark -
- (void)makeConstraints{
    [self.tableView mas_makeConstraints:^(MASConstraintMaker *make) {
        make.top.mas_equalTo(self.view);
        make.width.mas_equalTo(self.view);
        make.left.mas_equalTo(self.view);
        make.bottom.mas_equalTo(self.bottomBtns.mas_top);
    }];

}

- (void)setBarButtonItems{
    UIBarButtonItem *left = [[UIBarButtonItem alloc]initWithImage:[UIImage imageNamed:@"箭头_左"] style:UIBarButtonItemStylePlain target:self action:@selector(backButtonAct)];
    self.navigationItem.leftBarButtonItem = left;
    
    UIButton *r = [UIButton buttonWithType:UIButtonTypeCustom];
    [r setFrame:CGRectMake(0, 0, 30, 30)];
    [r setImage:[UIImage imageNamed:@"更多"] forState:UIControlStateNormal];
    [r addTarget:self action:@selector(popoverButtonAct:) forControlEvents:UIControlEventTouchUpInside];
    UIBarButtonItem *right = [[UIBarButtonItem alloc]initWithCustomView:r];
    
    UIButton *p = [UIButton buttonWithType:UIButtonTypeCustom];
    [p setFrame:CGRectMake(0, 0, 30, 30)];
    [p setImage:[UIImage imageNamed:@"电话h"] forState:UIControlStateNormal];
    [p addTarget:self action:@selector(phoneButtonAct:) forControlEvents:UIControlEventTouchUpInside];
    UIBarButtonItem *phone = [[UIBarButtonItem alloc]initWithCustomView:p];
    
    UIButton *s = [UIButton buttonWithType:UIButtonTypeCustom];
    [s setFrame:CGRectMake(0, 0, 30, 30)];
    [s setImage:[UIImage imageNamed:@"分享"] forState:UIControlStateNormal];
    [s addTarget:self action:@selector(shareButtonAct:) forControlEvents:UIControlEventTouchUpInside];
    UIBarButtonItem *share = [[UIBarButtonItem alloc]initWithCustomView:s];
    
    self.navigationItem.rightBarButtonItems = @[right, phone, share];
}

- (void)hiddenBarButtonItems{
    self.navigationItem.leftBarButtonItem = nil;
    self.navigationItem.rightBarButtonItems = nil;
}

#pragma mark - network
- (void)loadData {
    self.tableView.state = WLKTViewStateLoading;
    WLKTCourseDetailApi *api = [[WLKTCourseDetailApi alloc] initWithCourseId:_courseId latitude:[NSString stringWithFormat:@"%f", self.currentCoordinate.latitude] longitude:[NSString stringWithFormat:@"%f", self.currentCoordinate.longitude]];
    @weakify(self)
    [api startWithCompletionBlockWithSuccess:^(__kindof YTKBaseRequest *request) {
        self.tableView.state = WLKTViewStateNormal;
        @strongify(self)
        _courseDetailData = [WLKTCourseDetailData modelWithJSON:request.responseJSONObject[@"result"]];
        self.course = _courseDetailData.list;
        NSArray *area = [NSArray modelArrayWithClass:[WLKTCoursePriceRange class] json:request.responseJSONObject[@"result"][@"area"]];
        NSArray *point = [NSArray modelArrayWithClass:[WLKTAddress class] json:request.responseJSONObject[@"result"][@"point"]];
        _course.area = [NSMutableArray arrayWithArray:area];
        _course.point = [point copy];
        _photoArray = [NSArray arrayWithArray:_courseDetailData.param.photo];
        _teacherArr = [NSArray arrayWithArray:_courseDetailData.param.teacher];
        _about_listArr = [NSArray arrayWithArray:_courseDetailData.param.about_list];
        _hotSchoolArr = [NSArray arrayWithArray:_courseDetailData.param.hot_school];
        NSMutableArray *photos = [NSMutableArray array];
        for (WLKTCourseDetailNewPhoto *obj in _photoArray) {
            [photos addObject:obj.photo];
        }
        if (_photoArray.count) {
            [self prepareForPhotoBroswerWithImageArray:photos];
        }
        dispatch_async(dispatch_get_main_queue(), ^{
            [self.tableView.mj_header endRefreshing];
            self.title = _course.coursename;
            if (_course.shoucang.intValue == 1) {//已收藏
                [self.bottomBtns.collectBtn setTitle:@" 已收藏" forState:UIControlStateNormal];
                [self.bottomBtns.collectBtn setImage:[UIImage imageNamed:@"课程详情已收藏"] forState:UIControlStateNormal];
            }
            self.tableView.tableFooterView = self.tableViewFooterlabel;
            [self.tableView reloadData];
        });
        
    } failure:^(__kindof YTKBaseRequest *request) {
        [self.tableView.mj_header endRefreshing];
        self.tableView.state = WLKTViewStateError;
        WS(weakSelf);
        self.tableView.emptyButtonClickBlock = ^{
            [weakSelf loadData];
        };
    }];
}

#pragma mark - table view
- (NSInteger)numberOfSectionsInTableView:(UITableView *)tableView{
    if (self.courseDetailData) {
        return self.sectionTitleArr.count;
    }
    return 0;
}

- (NSInteger)tableView:(UITableView *)tableView numberOfRowsInSection:(NSInteger)section{
    switch (section) {
        case 1://上课地点
            return self.course.point.count;
        case 5://课程详情
            return self.detailImageArr.count;
        case 8://评价
            if (self.courseDetailData.param.course_comment.comment.count) {
                return self.courseDetailData.param.course_comment.comment.count + 1;
            }
            else{
                return 0;
            }
        case 9://问答
            if (self.courseDetailData.param.course_ques.ques.count) {
                NSInteger count = self.courseDetailData.param.course_ques.ques.count;
                for (WLKTCourseDetailNewCourse_quesList *obj in self.courseDetailData.param.course_ques.ques) {
                    if (obj.answer.answer.length > 0) {
                        count++;
                    }
                }
                return count;
            }
            else{
                return 0;
            }
        case 10://投诉
            if (self.courseDetailData.param.course_complaint.complaint.count) {
                NSInteger count = self.courseDetailData.param.course_complaint.complaint.count;
                for (WLKTCourseDetailNewCourse_complaintList *obj in self.courseDetailData.param.course_complaint.complaint) {
                    if (obj.answer.result.length > 0) {
                        count++;
                    }
                }
                return count;
            }
            else{
                return 0;
            }
        default:
            return 1;
    }
}

- (CGFloat)tableView:(UITableView *)tableView heightForRowAtIndexPath:(NSIndexPath *)indexPath{
    if (indexPath.section == 0) {
        if (ScreenWidth > 375) {
            return 380;
        }
        else if (ScreenWidth == 375){
            return 360;
        }
        else{
            return 320;
        }

    }
    if (indexPath.section == 1) {//上课地点
        return 60;
    }
    if (indexPath.section == 2) {//隐私电话
        return 65;
    }
    if (indexPath.section == 3 && self.courseDetailData.param.photo.count) {//官方相册
        return 120;
    }
    if (indexPath.section == 4) {//全景校区
        return 122 *ScreenRatio_6;
    }
    if (indexPath.section == 5) {//课程详情
        for (NSNumber *obj in self.isCourseDetailMoreDic.allKeys) {
            if (indexPath.row == obj.integerValue) {
                NSString *s = [NSString stringWithFormat:@"%ld", indexPath.row];
                if (self.isCourseDetailMoreDic[obj].boolValue) {
                    return ((NSNumber *)(self.courseDetailHeightDic[s])).floatValue + 55;
                }
                else{
                    return ((NSNumber *)(self.courseDetailHeightDic[s])).floatValue > 100.0 ? 150 : 100;
                }
            }
        }
        
    }
    if (indexPath.section == 6 && self.courseDetailData.param.teacher.count) {//授课老师
        return 130;
    }
    if (indexPath.section == 7) {//学校
        return 50;
    }
    if (indexPath.section == 8 && self.courseDetailData.param.course_comment.comment.count) {//评价
        if (indexPath.row == 0) {
            return 65;
        }
        else {
            WLKTCourseDetailNewCourse_commentItem *item = self.courseDetailData.param.course_comment.comment[indexPath.row - 1];
            CGFloat h = 120 *ScreenRatio_6;
            if (item.thumb_picture.count) {
                h += ((item.thumb_picture.count + 2) /3)* 115 * ScreenRatio_6;
            }
            return h;
        }
    }
    if (indexPath.section == 9 && self.courseDetailData.param.course_ques.ques.count) {//问答
        return 65;
    }
    if (indexPath.section == 10 && self.courseDetailData.param.course_complaint.complaint.count) {//投诉
        WLKTCourseDetailNewCourse_complaint *complaint = self.courseDetailData.param.course_complaint;
        NSMutableArray *arr = [NSMutableArray arrayWithCapacity:5];
        for (int i = 0; i < complaint.complaint.count; i++) {
            [arr addObject:complaint.complaint[i].question_item];
            if (complaint.complaint[i].answer.result.length > 0) {//有回复
                [arr addObject:complaint.complaint[i].answer];
            }
        }
        if ([arr[indexPath.row] isKindOfClass:[WLKTCourseDetailNewCourse_complaintComItem class]]) {
            WLKTCourseDetailNewCourse_complaintComItem *item = arr[indexPath.row];
            CGFloat h = 65 *ScreenRatio_6;
            if (item.thumb_picture.count) {
                h += ((item.thumb_picture.count + 2) /3)* 115 * ScreenRatio_6;
            }
            return h;
        }
        return 65 *ScreenRatio_6;
    }
    if (indexPath.section == 11) {//相关课程
        return ((self.courseDetailData.param.about_list.count + 1) / 2) * 155 *ScreenRatio_6;
    }
    if (indexPath.section == 12) {//还看了
        return self.courseDetailData.param.hot_school.count * 19 *ScreenRatio_6;
    }
    return 0;
}

- (CGFloat)tableView:(UITableView *)tableView heightForHeaderInSection:(NSInteger)section{
    if (section == 0 || section == 2 || section == 7) {
        return 0.01;
    }
    return 44;
}

- (CGFloat)tableView:(UITableView *)tableView heightForFooterInSection:(NSInteger)section{
    if (section == 3 || section == 5 || section == 8 || section == 9 || section == 11) {//官方相册, 课程详情, 评价, 问答, 相关课程
        return 0.5;
    }
    if (section == 12) {//还看了
        return 0.01;
    }
    return 5;
}

- (UIView *)tableView:(UITableView *)tableView viewForHeaderInSection:(NSInteger)section{
    if (section == 0 || section == 2 || section == 7) {
        return nil;
    }
    UIView *v = [[UIView alloc]initWithFrame:CGRectMake(0, 0, ScreenWidth, 44)];
    UILabel *titleLabel = [UILabel new];
    titleLabel.font = [UIFont systemFontOfSize:14 *ScreenRatio_6];
    titleLabel.textColor = KMainTextColor_3;
    titleLabel.text = self.sectionTitleArr[section];
    [v addSubview:titleLabel];
    [titleLabel mas_makeConstraints:^(MASConstraintMaker *make) {
        make.left.mas_equalTo(v).offset(10 *ScreenRatio_6);
        make.centerY.mas_equalTo(v);
    }];
    
    UIButton *detailBtn = [UIButton new];
    if (section == 8) {
        [detailBtn setImage:[UIImage imageNamed:@"评价L"] forState:UIControlStateNormal];
        [detailBtn setTitle:@" 我要评价" forState:UIControlStateNormal];
    }
    if (section == 9) {
        [detailBtn setImage:[UIImage imageNamed:@"问答L"] forState:UIControlStateNormal];
        [detailBtn setTitle:@" 我来问问" forState:UIControlStateNormal];
    }
    if (section == 10) {
        [detailBtn setImage:[UIImage imageNamed:@"投诉L"] forState:UIControlStateNormal];
        [detailBtn setTitle:@" 我要投诉" forState:UIControlStateNormal];
    }
    [detailBtn setTitleColor:UIColorHex(33c4da) forState:UIControlStateNormal];
    detailBtn.titleLabel.font = [UIFont systemFontOfSize:9 * ScreenRatio_6];
    detailBtn.tag = section;
    [detailBtn addTarget:self action:@selector(tableViewHeaderLevel_3_Act:) forControlEvents:UIControlEventTouchUpInside];
    [v addSubview:detailBtn];
    if (section == 8 || section == 9 || section == 10) {
        detailBtn.hidden = NO;
    }
    else{
        detailBtn.hidden = YES;
    }
    [detailBtn mas_makeConstraints:^(MASConstraintMaker *make) {
        make.left.mas_equalTo(titleLabel.mas_right).offset(15);
        make.centerY.mas_equalTo(v);
    }];
    
    WLKTExchangeButton *btn = [WLKTExchangeButton new];
    [btn setImage:[UIImage imageNamed:@"进入-拷贝"] forState:UIControlStateNormal];
    [btn setTitle:@"    " forState:UIControlStateNormal];
    [btn setTitleColor:KMainTextColor_9 forState:UIControlStateNormal];
    btn.titleLabel.font = [UIFont systemFontOfSize:12 * ScreenRatio_6];
    btn.tag = section;
    [v addSubview:btn];
    if (section == 7 || section == 8 || section == 9 || section == 10) {
        btn.hidden = NO;
    }
    else{
        btn.hidden = YES;
    }
    [btn mas_makeConstraints:^(MASConstraintMaker *make) {
        make.right.mas_equalTo(v.mas_right).offset(-10);
        make.height.centerY.mas_equalTo(v);
    }];

    if (section == 8) {
        if (self.courseDetailData.param.course_comment.comment.count) {
            [btn addTarget:self action:@selector(tableViewHeaderAct:) forControlEvents:UIControlEventTouchUpInside];
        }
        else{
            titleLabel.text = @"暂无评价";
            detailBtn.hidden = YES;
            [btn setTitle:@"去评价 " forState:UIControlStateNormal];
            [btn addTarget:self action:@selector(tableViewHeaderLevel_3_Act:) forControlEvents:UIControlEventTouchUpInside];
        }
    }
    if (section == 9) {
        if (self.courseDetailData.param.course_ques.total) {
            [btn addTarget:self action:@selector(tableViewHeaderAct:) forControlEvents:UIControlEventTouchUpInside];
        }
        else{
            detailBtn.hidden = YES;
            titleLabel.text = @"暂无问答";
            [btn setTitle:@"去提问 " forState:UIControlStateNormal];
            [btn addTarget:self action:@selector(tableViewHeaderLevel_3_Act:) forControlEvents:UIControlEventTouchUpInside];
        }
    }
    if (section == 10) {
        if (self.courseDetailData.param.course_complaint.total) {
            [btn addTarget:self action:@selector(tableViewHeaderAct:) forControlEvents:UIControlEventTouchUpInside];
        }
        else{
            detailBtn.hidden = YES;
            titleLabel.text = @"暂无投诉";
            [btn setTitle:@"去投诉 " forState:UIControlStateNormal];
            [btn addTarget:self action:@selector(tableViewHeaderLevel_3_Act:) forControlEvents:UIControlEventTouchUpInside];
        }
    }
    return v;
}

- (UIView *)tableView:(UITableView *)tableView viewForFooterInSection:(NSInteger)section{
    UIView *v = [[UIView alloc]initWithFrame:CGRectMake(0, 0, ScreenWidth, 5)];
    v.backgroundColor = separatorView_color;
    return v;
}

- (UITableViewCell *)tableView:(UITableView *)tableView cellForRowAtIndexPath:(NSIndexPath *)indexPath{
    UITableViewCell *cell = [UITableViewCell new];
    if (indexPath.section == 0 && self.course) {
        WLKTCourseDetailHeadCell *hc = [[WLKTCourseDetailHeadCell alloc]init];
        [hc setCellData:self.course];
        cell = hc;
    }
    if (indexPath.section == 1) {//上课地点
        WLKTCourseDetailLocationCell *lc = [[WLKTCourseDetailLocationCell alloc]init];
        if (self.course.point.count) {
            [lc setCellData:self.course index:indexPath.row currentCoordinate:self.currentCoordinate];
        }
        cell = lc;
    }
    if (indexPath.section == 2) {//隐私电话
        WLKTCourseDetailPrivatePhoneCell *ppc = [[WLKTCourseDetailPrivatePhoneCell alloc]init];
        cell = ppc;
    }
    if (indexPath.section == 3 && self.photoArray.count) {//官方相册
        WLKTCourseDetailPhotoCell *pc = [[WLKTCourseDetailPhotoCell alloc]initWithImageArray:self.photoArray];
        pc.delegate = self;
        cell = pc;
    }
    if (indexPath.section == 4) {//全景校区
        WLKTCourseDetailVRPhotoCell *vrc = [[WLKTCourseDetailVRPhotoCell alloc]initWithListArr:self.courseDetailData.param.vrlist];
        vrc.delegate = self;
        cell = vrc;
    }
    if (indexPath.section == 5) {//课程详情
        WLKTCourseDetailIntroCell *ic = [tableView dequeueReusableCellWithIdentifier:@"WLKTCourseDetailIntroCell"];
        if (ic == nil) {
            ic = [[WLKTCourseDetailIntroCell alloc]initWithStyle:UITableViewCellStyleDefault reuseIdentifier:@"WLKTCourseDetailIntroCell"];
        }
        ic.delegate = self;
        if (self.course) {
            [ic setCellData:self.course image:self.detailImageArr[indexPath.row] titleColor:self.detailTitleColorArr[indexPath.row] title:self.introTitleArr[indexPath.row] detail:self.courseDetailHtmlStringArr[indexPath.row] index:indexPath.row isMore:self.isCourseDetailMoreDic[@(indexPath.row)].boolValue];
        }
        cell = ic;
    }
    if (indexPath.section == 6) {//授课老师
        if (self.teacherArr.count) {
            WLKTCourseDetailTeacherCell *tc = [[WLKTCourseDetailTeacherCell alloc]initWithTeacherArray:self.teacherArr];
            tc.delegate = self;
            cell = tc;
        }
    }
    if (indexPath.section == 7) {//学校
        WLKTCourseDetailSchoolCell *sc = [[WLKTCourseDetailSchoolCell alloc]init];
        [sc setCellData:self.course];
        cell = sc;
    }
    if (indexPath.section == 8) {//评价
        if (indexPath.row == 0) {
            WLKTCourseDetailEvaluationStarBtnCell *esbc = [[WLKTCourseDetailEvaluationStarBtnCell alloc]initWithStar:self.courseDetailData.param.course_comment.star];
            esbc.delegate = self;
            cell = esbc;
        }
        else if (self.courseDetailData.param.course_comment.comment.count){
            WLKTCourseDetailEvaluationCell *ec = [[WLKTCourseDetailEvaluationCell alloc]init];
            [ec setCellData:self.courseDetailData.param.course_comment.comment[indexPath.row - 1]];
            cell = ec;
        }
    }
    if (indexPath.section == 9 && self.courseDetailData.param.course_ques.total) {//问答
        WLKTCourseDetail_Q_A_cell *qac = [[WLKTCourseDetail_Q_A_cell alloc]init];
        [qac setCellData:self.courseDetailData.param.course_ques index:indexPath.row];
        cell = qac;
    }
    if (indexPath.section == 10 && self.courseDetailData.param.course_complaint.total) {//投诉
        WLKTCourseDetialComplaintCell *cc = [[WLKTCourseDetialComplaintCell alloc]init];
        [cc setCellData:self.courseDetailData.param.course_complaint index:indexPath.row];
        cell = cc;
    }
    if (indexPath.section == 11 && self.courseDetailData.param.about_list.count) {//相关课程
        WLKTCourseDetailOtherCourseCell *occ = [[WLKTCourseDetailOtherCourseCell alloc]initWithData:self.courseDetailData.param.about_list];
        occ.delegate = self;
        cell = occ;
    }
    if (indexPath.section == 12 && self.courseDetailData.param.hot_school.count) {//还看了
        WLKTCourseDetailMoreBrowseCell *mbc = [[WLKTCourseDetailMoreBrowseCell alloc]initWithData:self.courseDetailData.param.hot_school];
        mbc.delegate = self;
        cell = mbc;
    }
    cell.selectionStyle = UITableViewCellSelectionStyleNone;
    return cell;
}

- (void)scrollViewDidScroll:(UIScrollView *)scrollView{
    self.backButton.alpha = 1 - self.currentAlpha;
    self.shareButton.alpha = 1 - self.currentAlpha;
    self.popoverButton.alpha = 1 - self.currentAlpha;
    if (self.currentAlpha > 0.1) {
        [self.navigationController setNavigationBarHidden:NO animated:NO];
    }
    else{
        [self.navigationController setNavigationBarHidden:YES animated:NO];
    }
    self.navigationController.navigationBar.titleTextAttributes = @{NSForegroundColorAttributeName: [UIColor colorWithWhite:0.2 alpha:self.currentAlpha]};
    [self.navigationController.navigationBar setBackgroundImage:[UIImage imageWithColor:[UIColor colorWithWhite:1.0 alpha:self.currentAlpha >= 1 ? 0.99 : self.currentAlpha]] forBarMetrics:UIBarMetricsDefault];
    [self.navigationController.navigationBar setShadowImage:[UIImage imageWithColor:[UIColor colorWithWhite:14/16.0 alpha:self.currentAlpha >= 1 ? 0.99 : self.currentAlpha]]];
    self.currentAlpha = scrollView.contentOffset.y / 200.0 >= 1.0 ? 1 : scrollView.contentOffset.y / 200.0;
    if (self.currentAlpha >= 1) {
        [self setBarButtonItems];
    }
    else{
        [self hiddenBarButtonItems];
    }
    //set head indexes color
    if (self.courseDetailData && self.tableView.numberOfSections >= 5) {
        
        if (scrollView.contentOffset.y + self.insetY >= [self.tableView rectForSection:4].origin.y && scrollView.contentOffset.y + self.insetY < [self.tableView rectForSection:5].origin.y) {//vr
            self.headIndexes.hidden = NO;
            [self.headIndexes setItemColorAtIndex:0];
        }
        if (scrollView.contentOffset.y + self.insetY >= [self.tableView rectForSection:5].origin.y && scrollView.contentOffset.y + self.insetY < [self.tableView rectForSection:8].origin.y) {//课程详情
            self.headIndexes.hidden = NO;
            [self.headIndexes setItemColorAtIndex:1];
        }
        if (scrollView.contentOffset.y + self.insetY >= [self.tableView rectForSection:8].origin.y && scrollView.contentOffset.y + self.insetY < [self.tableView rectForSection:11].origin.y) {//评价
            self.headIndexes.hidden = NO;
            [self.headIndexes setItemColorAtIndex:2];
        }
        if (scrollView.contentOffset.y + self.insetY >= [self.tableView rectForSection:11].origin.y) {//相关课程
            self.headIndexes.hidden = NO;
            [self.headIndexes setItemColorAtIndex:3];
        }
        if (scrollView.contentOffset.y + self.insetY < [self.tableView rectForSection:4].origin.y){
            self.headIndexes.hidden = YES;
        }

    }
}

