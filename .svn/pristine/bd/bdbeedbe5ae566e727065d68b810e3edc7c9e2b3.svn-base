#import "YTKRequest.h"

@interface WLKTActivityListApi : YTKRequest

/*
 * aid 活动分类筛选参数名称 参数值:分类的id
 * time 时间筛选参数名称 参数值:今天,明天,一周内,一个月内
 * price 价格筛选参数名称 参数值:收费,免费
 * actstatus 活动状态筛选 参数值:进行中,即将开始,已结束
 * distance 距离筛选参数名称 参数值:离我最近 当传递该参数的时候需要再传递当前定位的经度lng和纬度lat
 */

- (instancetype)initWithParam:(NSDictionary *)para;

@end
